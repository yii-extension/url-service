{
    "name": "yii-extension/url-service",
    "type": "library",
    "description": "Url service",
    "keywords": [
        "yii3",
        "url",
        "service"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/router": "^3.0@dev"
    },
    "require-dev": {
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Service\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Service\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
